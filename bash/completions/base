COMPLETIONS_PATH=$BASE_PATH/completions

source $COMPLETIONS_PATH/make_completion_wrapper
source $COMPLETIONS_PATH/git
complete -C $COMPLETIONS_PATH/pharo_completion -o default pharo
source $COMPLETIONS_PATH/rake-completion # Rake (cached) completion
source $COMPLETIONS_PATH/teamocil
source $COMPLETIONS_PATH/lein-completion
